# GNU Make solution makefile autogenerated by Premake

ifndef config
  config=debug_native
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_native)
  undoredotests_config = debug_native
endif
ifeq ($(config),release_native)
  undoredotests_config = release_native
endif

PROJECTS := undoredotests

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

undoredotests: 
ifneq (,$(undoredotests_config))
	@echo "==== Building undoredotests ($(undoredotests_config)) ===="
	@${MAKE} --no-print-directory -C . -f undoredotests.make config=$(undoredotests_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f undoredotests.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_native"
	@echo "  release_native"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   undoredotests"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
